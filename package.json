    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "lerna": "^3.22.1",
    "typescript": "^3.9.7",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.5.0",
    "husky": "^4.2.5",
    "rimraf": "^3.0.2"
  }
}
