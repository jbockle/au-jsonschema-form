{
  "name": "au-jsonschema-form",
  "private": true,
  "scripts": {
    "build": "lerna run build",
    "clean": "lerna clean -y",
    "postclean": "rimraf packages/**/package-lock.json",
    "test": "lerna exec -- npm test",
    "lint": "lerna run lint --parallel",
    "start": "lerna exec --scope examples -- npm start",
