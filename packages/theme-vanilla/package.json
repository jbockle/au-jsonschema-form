{
  "name": "@au-jsonschema-form/theme-vanilla",
  "author": "jbockle",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist/*",
    "clean": "rimraf dist/*",
    "build:tsc": "tsc",
    "build:static": "cpx src/**/*.{html,css} dist",
    "build": "npm-run-all build:*",
    "prebuild-watch": "npm run clean",
    "build-watch": "npm-run-all --parallel \"build:* -- --watch\"",
    "test": "nyc mocha",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "@au-jsonschema-form/core": "0.1.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "typescript": "^3.7.5",
    "cpx": "^1.5.0",
    "rimraf": "^3.0.0",
    "eslint": "^6.8.0",
    "npm-run-all": "^4.1.5",
    "mocha": "^7.0.1",
    "chai": "^4.2.0",
    "nyc": "^15.0.0",
    "ts-node": "^8.6.2"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**/*.ts"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "all": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  }
}
