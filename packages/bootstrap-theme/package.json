{
  "name": "@aujsf/bootstrap-theme",
  "description": "Bootstrap 4 Theme for ajsf",
  "author": "jbockle",
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist/*",
    "clean": "rimraf dist/* coverage/*",
    "build:tsc": "tsc -b tsconfig.build.json",
    "build:static": "cpx src/**/*.{html,css} dist",
    "build": "npm-run-all build:*",
    "prebuild-watch": "npm run clean",
    "build-watch": "npm-run-all --parallel \"build:* -- --watch\"",
    "pretest": "npm run clean",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "@aujsf/core": "^0.0.1"
  },
  "devDependencies": {
    "aurelia-framework": "^1.3.1",
    "aurelia-logging": "^1.5.2",
    "bootstrap": "^4.5.0",
    "braces": ">=2.3.1",
    "cpx": "^1.5.0",
    "eslint": "^7.5.0",
    "jquery": "^3.5.1",
    "jsonpointerx": "^1.0.29",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  }
}
